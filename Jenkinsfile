pipeline{
  agent any
  stages {
    
    stage('build'){
      steps{ 
        sh 'echo "we are here build"'
      }
    }
    
    stage('test'){
      steps{ 
        sh 'echo "we are here test"'
      }
    }
        
    stage('deploy'){
      steps{ 
        sh 'echo "we are here deploy"'
      }
    }
    
  }
}
